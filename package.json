    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "cypress": "^13.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "jsdom": "^24.1.1",
    "terser": "^5.4.0",
    "typescript": "^5.5.2",
